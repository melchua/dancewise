<%= search_form_for @q,  
    data: { controller: "places", action: "google-maps-callback@window->places#initMap" } do |f| %>

      <%= f.label :location %>
    <%= f.search_field :name_cont, placeholder: "Name contains" %> &emsp;

    <%= f.label :location %>
    <%= f.search_field :address_or_city_or_state_or_country_or_continent_cont_all, data: { target: "places.field" } %> &emsp;

    <%= f.label :dance_style %>
    <%= f.collection_select :dance_styles_id_eq, DanceStyle.all, :id, :name, :multiple => true, :include_blank => "All dance styles" %> &emsp;

    <%= f.label "Event Type" %>
    <%= f.collection_select :event_type_id_eq, EventType.all, :id, :name, :multiple => true, :include_blank => "All event types" %> &emsp;
    <br>
    <br>

    <%= f.select(:event_month_eq, Date::MONTHNAMES[1..12], {:prompt => 'Select Month', include_blank: true}) %>

    <%= f.select(:event_year_eq, Date.today.year-2 .. Date.today.year+3, {:prompt => 'Select Year', include_blank: true}) %>

    <%= f.submit "Search!" %>
<% end %>





