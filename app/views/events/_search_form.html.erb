<%= search_form_for @q,  
    data: { controller: "places", action: "google-maps-callback@window->places#initCityMap" } do |f| %>

    <%= f.label :name_cont %>
    <%= f.search_field :name_cont %>

    <%= f.label :location %>
    <%= f.search_field :address_i_cont_any, data: { target: "places.field" } %>
 
    <%= f.label :dance_style %>
    <%= f.collection_select :dance_styles_id_eq, DanceStyle.all, :id, :name, include_blank: true %>

    <%= f.submit "Search!" %>

<% end %>




